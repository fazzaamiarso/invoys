  "scripts": {
    "build": "next build",
    "build:ci": "cross-env APP_ENV=test next build",
    "dev": "next dev",
    "dev:test": "cross-env APP_ENV=test NODE_ENV=test next dev -p 8080",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "start:ci": "next start -p 8080",
    "prepare": "husky install",
    "db:push": "prisma db push",
    "db:seed": "prisma migrate reset --skip-generate --skip-seed && prisma db push && prisma db seed",
    "db:seed-test": "dotenv -e .env.test -- prisma migrate reset --skip-generate --skip-seed --force && dotenv -e .env.test -- prisma db push && dotenv -e .env.test -- prisma db seed",
    "db:studio": "prisma studio",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "start-server-and-test dev:test 8080 cy:open",
    "cy:test-run": "start-server-and-test dev:test 8080 cy:run",
    "test:unit": "vitest"
  },
